//keyframes
#miniatury img {
	animation: miniaturyAnimacja 4s;
	padding-left: 50px;
	}
	
/* Animacja miniatur */
@keyframes miniaturyAnimacja {
	from {
	 padding-left: 50px;
	}
	to {
	 padding-left: 0px;
	 }}

@keyframes slide-in {
  from {
    transform: translateX(0%);
  }

  to {
    transform: translateX(100%);
  }
}

@keyframes identifier {
  0% {
    top: 0;
    left: 0;
  }
  30% {
    top: 50px;
  }
  68%,
  72% {
    left: 50px;
  }
  100% {
    top: 100px;
    left: 100%;
  }
}